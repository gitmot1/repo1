[project]
	description = repo1
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/gitmot1
[access "refs/for/refs/*"]
	read = group user/gitmot1
	push = group user/gitmot1
	label-Code-Review = -2..+2 group user/gitmot1
	label-Verified = -1..+1 group user/gitmot1
	rebase = group user/gitmot1
	removeReviewer = group user/gitmot1
	submit = group user/gitmot1
[access "refs/heads/*"]
	read = group user/gitmot1
	create = group user/gitmot1
	push = +force group user/gitmot1
	pushMerge = group user/gitmot1
[access "refs/tags/*"]
	push = +force group user/gitmot1
	pushTag = group user/gitmot1
	pushSignedTag = group user/gitmot1
